<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gdnf.lazy.tips">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tips"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tips"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.AboutActivity" />
        <activity android:name=".activitys.AddTipsActivity" />
        <activity android:name=".activitys.ReminderActivity" />
        <activity android:name=".activitys.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>